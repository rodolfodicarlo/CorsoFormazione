					"name": "GetToken",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Analizza la risposta JSON\r",
									"var responseJson = pm.response.json();\r",
									"\r",
									"// Verifica che la risposta contenga l'attributo 'payload'\r",
									"pm.test(\"Verifico che la risposta contenga il payload\", function () {\r",
									"    pm.expect(responseJson).to.have.property('payload');\r",
									"});\r",
									"\r",
									"// Estrai il valore di accessToken dal payload e memorizzalo nella variabile di collezione\r",
									"if (responseJson.payload) {\r",
									"    var accessTokenValue = responseJson.payload.accessToken;\r",
									"    var refreshTokenValue = responseJson.payload.refreshToken;\r",
									"    \r",
									"    // Verifica che accessToken sia presente nel payload\r",
									"    pm.test(\"Verifico che il payload contenga accessToken e refreshToken\", function () {\r",
									"        pm.expect(responseJson.payload).to.have.property('accessToken');\r",
									"        pm.expect(responseJson.payload).to.have.property('refreshToken');\r",
									"    });\r",
									"\r",
									"    // Memorizza il valore di accessToken nella variabile di collezione chiamata 'accessToken'\r",
									"    pm.collectionVariables.set('accessTokenClient', accessTokenValue);\r",
									"    pm.collectionVariables.set('refreshTokenClient', refreshTokenValue);\r",
									"\r",
									"    // Log del valore di accessToken\r",
									"    console.log('accessTokenClient:', accessTokenValue);\r",
									"    console.log('refreshTokenClient:', refreshTokenValue);\r",
									"} else {\r",
									"    console.log('Il payload non è presente nella risposta');\r",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
